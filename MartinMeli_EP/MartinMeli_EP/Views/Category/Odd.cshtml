@{
    ViewBag.Title = "Odd";
}

<div class="col-lg-12">
    @foreach (var article in ViewBag.articles)
    {
        if (article.categoryId == 4)
        {
            Html.Partial("~/Views/Category/CategoryPartial.cshtml", new Data.article() { headline = article.headline, articleId = article.articleId, imageURL = article.imageURL, description = article.description, journalist = article.journalist });
        }
    }
</div>